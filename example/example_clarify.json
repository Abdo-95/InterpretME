{
  "Endpoint": "https://labs.tib.eu/sdm/clarify-kg-7/sparql",
  "Type": "CLARIFY-SLCG_Patient",
  "Index_var": "EHR",
  "Independent_variable": {
    "EHR": "?EHR1 a <http://clarify2020.eu/vocab/LCPatient>. ?EHR1  <http://clarify2020.eu/vocab/has_LC_SLCG_ID> ?EHR. \n ",
    "Gender": "?EHR <http://clarify2020.eu/vocab/sex> ?Gender. \n ",
    "AgeCategory": "?EHR <http://clarify2020.eu/vocab/age> ?Age. bind( if(?Age>50,'OLDER','YOUNGER') as ?AgeCategory ) \n ",
    "Stages": "OPTIONAL {?EHR <http://clarify2020.eu/vocab/hasDiagnosis> ?o1 . ?o1 <http://clarify2020.eu/vocab/hasDiagnosisStage> ?Stages.}. \n",
    "Smoking_habits": "?EHR <http://clarify2020.eu/vocab/hasSmokingHabit> ?Smoking_habits.  FILTER (?Smoking_habits NOT IN (<http://clarify2020.eu/entity/UNK>)) . \n",
    "CancerType": "?EHR <http://clarify2020.eu/vocab/hasFamilyHistory> ?o . ?o <http://clarify2020.eu/vocab/hasFamilyCancerType> ?CancerType . FILTER(!bound(?CancerType) || ?CancerType  IN (<http://clarify2020.eu/entity/Prostate>, <http://clarify2020.eu/entity/Breast>, <http://clarify2020.eu/entity/Lung>, <http://clarify2020.eu/entity/Colorrectal>, <http://clarify2020.eu/entity/Head_and_neck>, <http://clarify2020.eu/entity/Esophagogastric>, <http://clarify2020.eu/entity/Uterus/cervical>, <http://clarify2020.eu/entity/Skin_no_melanoma> ))\n",
    "FamilyDegree": "?EHR <http://clarify2020.eu/vocab/hasFamilyHistory> ?o . ?o <http://clarify2020.eu/vocab/familyRelationDegree> ?FamilyDegree . FILTER (!bound(?FamilyDegree) || ?FamilyDegree IN (<http://clarify2020.eu/entity/Male_First_Degree>,<http://clarify2020.eu/entity/Female_First_Degree>, <http://clarify2020.eu/entity/Male_Second_Degree>, <http://clarify2020.eu/entity/Female_Second_Degree>, <http://clarify2020.eu/entity/Male_Third_Degree>, <http://clarify2020.eu/entity/Female_Third_Degree>)) \n"
  },
  "Dependent_variable": {
    "Biomarker": "?EHR <http://clarify2020.eu/vocab/hasBio> ?Biomarker ."
  },
  "Constraints": [
    {
      "name": "Afatinib is not recommended for NSLC EGFR negative (hasDrug)",
      "inverted": true,
      "shape_schema_dir": "example/shapes/clarify/Afatinib_EGFR_treatmentline_hasDrug",
      "target_shape": "EGFRNegativePatientInverted"
    },
    {
      "name": "Afatinib is not recommended for NSLC EGFR negative (maintenance)",
      "inverted": true,
      "shape_schema_dir": "example/shapes/clarify/Afatinib_EGFR_treatmentline_maintenance",
      "target_shape": "EGFRNegativePatientInverted"
    },
    {
      "name": "Gefitinib is not recommended for NSLC EGFR negative (hasDrug)",
      "inverted": true,
      "shape_schema_dir": "example/shapes/clarify/Gefitinib_EGFR_treatmentline_hasDrug",
      "target_shape": "EGFRNegativePatientInverted"
    },
    {
      "name": "Gefitinib is not recommended for NSLC EGFR negative (maintenance)",
      "inverted": true,
      "shape_schema_dir": "example/shapes/clarify/Gefitinib_EGFR_treatmentline_maintenance",
      "target_shape": "EGFRNegativePatientInverted"
    }
  ],
  "classes": {
    "class0": "ALK",
    "class1": "Others"
  },
  "3_valued_logic": false,
  "sampling_strategy": "undersampling",
